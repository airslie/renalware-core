article.capd-regimes
  header
    h2 CAPD Regimes
    .supplemental
      = collection_count(capd_regimes)
    span.noprint
      = link_to t("btn.view_all"),
                patient_pd_capd_regimes_path(patient),
                class: "button secondary"

  = render "renalware/pd/capd_regimes", regimes: capd_regimes, patient: patient
